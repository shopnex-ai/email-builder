{
    "name": "@shopnex/editor-sample",
    "version": "0.0.5",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "development": {
                "import": "./src/index.ts",
                "types": "./src/index.ts"
            },
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "clean": "rimraf {dist,*.tsbuildinfo}",
        "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
        "preview": "vite preview",
        "prepublish": "pnpm clean && npm run build && npm run build:types"
    },
    "dependencies": {
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.15.10",
        "@mui/material": "^5.15.10",
        "@emailstudio/block-avatar": "workspace:*",
        "@emailstudio/block-button": "workspace:*",
        "@emailstudio/block-columns-container": "workspace:*",
        "@emailstudio/block-container": "workspace:*",
        "@emailstudio/block-divider": "workspace:*",
        "@emailstudio/block-heading": "workspace:*",
        "@emailstudio/block-html": "workspace:*",
        "@emailstudio/block-image": "workspace:*",
        "@emailstudio/block-spacer": "workspace:*",
        "@emailstudio/block-text": "workspace:*",
        "@emailstudio/document-core": "workspace:*",
        "@emailstudio/email-builder": "workspace:*",
        "highlight.js": "^11.9.0",
        "prettier": "^3.2.5",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-colorful": "^5.6.1",
        "zod": "^3.22.4",
        "zustand": "^4.5.1"
    },
    "devDependencies": {
        "@types/react": "19.1.0",
        "@types/react-dom": "19.1.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-simple-import-sort": "^12.0.0",
        "typescript": "^5.2.2",
        "vite": "^5.1.0"
    },
    "publishConfig": {
        "access": "public",
        "exports": {
            ".": {
                "import": "./dist/index.js",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    }
}